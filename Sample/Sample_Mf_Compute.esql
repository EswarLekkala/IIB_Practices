



CREATE COMPUTE MODULE Sample_Mf_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN

		CALL CopyEntireMessage();
		RETURN FALSE;
	END;


	CREATE PROCEDURE CopyEntireMessage()
	BEGIN
		DECLARE inref REFERENCE TO InputRoot.XMLNSC.EmployeeDetails.Emp;
		
		IF (inref.id =1) THEN
		set OutputRoot.XMLNSC.EmployeeDetails.Emp.id =	inref.id;
		ELSE
			set OutputRoot.XMLNSC.EmployeeDetails.Emp.id =	'';
		END IF;
		
		
		
		
		


	

END;
END MODULE;